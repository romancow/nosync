{
	"name": "@romancow/nosync",
	"version": "0.1.0",
	"description": "Stop iCloud from syncing designated files and folders",
	"main": "cjs/index.js",
	"module": "es6/index.js",
	"types": "types/index.d.ts",
	"bin": "cjs/cli.js",
	"files": [
		"es6/",
		"cjs/",
		"types/"
	],
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"clean": "rimraf ./es6/* ./cjs/* ./types/*",
		"build:es6": "tsc -p .",
		"build:cjs": "rollup -c",
		"prebuild": "npm run clean",
		"build": "npm run build:es6 && npm run build:cjs",
		"prepare": "npm run build"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/romancow/nosync.git"
	},
	"keywords": [
		"iCloud",
		"sync",
		"nosync",
		"symlink",
		"node_modules"
	],
	"author": "Sean Wilson",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/romancow/nosync/issues"
	},
	"homepage": "https://github.com/romancow/nosync#readme",
	"devDependencies": {
		"@types/fs-extra": "^8.0.1",
		"@types/node": "^12.12.6",
		"rimraf": "^3.0.0",
		"rollup": "^1.27.3",
		"rollup-plugin-consts": "^1.0.1",
		"rollup-plugin-executable": "^1.5.2",
		"typescript": "^3.7.2"
	},
	"dependencies": {
		"commander": "^4.0.1",
		"fs-extra": "^8.1.0"
	}
}
